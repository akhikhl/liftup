buildscript {
  apply plugin: 'maven'

  repositories {
    mavenLocal()
    mavenCentral()
  }
  
  dependencies {
    classpath 'org.akhikhl.underwork:underwork-gradle-plugin:0.0.1'
  }
}

import org.akhikhl.underwork.gradle.UnderworkPlugin

if (!project.plugins.findPlugin(UnderworkPlugin))
  project.apply(plugin: UnderworkPlugin)

